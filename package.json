{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.7",
    "razorpay": "^2.9.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.26.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "homepage": ".",
  "scripts": {
    "start": "node server/src/index.js",
    "dev": "concurrently --kill-others-on-fail --raw -c \"blue,green\" -n \"server,client\" -s \"true\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cross-env PORT=5000 node server/src/index.js",
    "dev:client": "cross-env PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "postbuild": "node -e \"require('fs').existsSync('server') && require('fs').existsSync('build') ? console.log('Build ready') : process.exit(1)\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm install --prefix server"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.3.0",
    "serve": "^14.2.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
